 ISSN = {19457731, 1945774X},
 URL = {https://www.jstor.org/stable/26529015},
 abstract = {Texas is the only US state that limits home equity borrowing to 80 percent of home value. This paper exploits this policy discontinuity around Texas’ interstate borders and uses a multidimensional regression discontinuity design framework to find that limits on home equity borrowing in Texas lowered the likelihood of mortgage default by about 1.5 percentage points for all mortgages and 4–5 percentage points for non-prime mortgages. Estimated non-prime mortgage default hazards within 25 to 100 miles on either side of the Texas border are about 20 percent smaller when crossing into Texas.},
 author = {Anil Kumar},
 journal = {American Economic Journal: Economic Policy},
 number = {1},
 pages = {268--297},
 publisher = {American Economic Association},
 title = {Do Restrictions on Home Equity Extraction Contribute to Lower Mortgage Defaults? Evidence from a Policy Discontinuity at the Texas Border},
 urldate = {2023-04-23},
 volume = {10},
 year = {2018}
}

